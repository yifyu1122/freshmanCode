void Sort(T *a, int size){
	for(int j=0;j<size-1;j++){
		for(int i=0;i<size-1;i++){
			if(a[i]>a[i+1]){
				swap(a[i], a[i+1]);
			}
		}
	}
	for(int i=0; i<size-1;i++){
		
	}
}

